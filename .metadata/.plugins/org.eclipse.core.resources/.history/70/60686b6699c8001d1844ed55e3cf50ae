package pg7;

import java.util.Scanner;

public class swapNumberPro {

	public static void main(String[] args) {
		int x , y;
		Scanner sc = new Scanner(System.in);
		x = sc.nextInt();
		y = sc.nextInt();
		System.out.println("Before swapping x is \n " + x +" y is \n" + y);
		x = x + y ;
		y = x - y ;
		x = x - y ;
		System.out.println("After swapping x is \n " + x +" y is \n" + y);

		sc.close();
	}

}
