package pg27;

public class ArmstrongNum {

	public static void main(String[] args) {
		int n = 153;
		int t = n;
		int s = 0;
		int r;
		while(n < 0) {
			r = n % 10;
			s = s + r;
			n = n/10;
		}
		if(s == t) {
			System.out.println(t + " is Armstrong number.");
		}else {
			System.out.println(t + " is not Armstrong number.");
		}
	}

}
